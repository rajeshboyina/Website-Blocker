{
    "manifest_version": 3,
    "name": "Website Blocker",
    "version": "1.0",
    "permissions": [
        "declarativeNetRequest",
        "storage",
        "tabs"
    ],
    "host_permissions": [
        "*://*/*"
        
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "background":
    {
        "service_worker":"background.js"
    }
}