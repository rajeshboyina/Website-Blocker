{
    "manifest_version": 3,
    "name": "Website Blocker",
    "version": "1.0",
    "permissions": [
        "declarativeNetRequest",
        "storage"
    ],
    "host_permissions": [
        "*://*.youtube.com/*",
        "*://*.googlevideo.com/*"
    ],
    "action": {
        "default_popup": "popup.html"
    }
}